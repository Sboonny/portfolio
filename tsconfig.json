{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "module": "ESNext",
    "lib": ["esnext", "dom", "dom.iterable"],
    "moduleResolution": "node",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "isolatedModules": true,
    "types": ["astro/env"],
    "allowSyntheticDefaultImports": true
  }
}
